@model Cwiczenia10.Models.StudentsModel
@{
    ViewData["Title"] = "Index";
}
<h1>Index</h1>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="striped">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StudentsList[0].FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StudentsList[0].LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StudentsList[0].Studies)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.StudentsList)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Studies)
            </td>
            <td>
            <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
        }
    </tbody>
</table>